<h4>Hackeados</h4>

    <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col"><br>Username</th>
            <th scope="col">Password</th>
          </tr>
        </thead>  
         <tbody>
         {{#each user}}
          <tr>
            
            <th scope="row">{{this.username}}</th>
            <td>{{this.password}}</td>
            
          </tr>
            {{/each}}
        </tbody>
    </table>


